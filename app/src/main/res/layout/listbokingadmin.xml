<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/intinyainti"
    android:layout_height="wrap_content">

  <androidx.cardview.widget.CardView
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      app:cardBackgroundColor="#FFFFFF"
      app:cardCornerRadius="16dp"
      app:cardElevation="4dp"
      app:cardUseCompatPadding="true"
      app:contentPadding="10dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


      <TextView

          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignParentTop="true"
          android:layout_marginStart="40dp"
          android:layout_marginLeft="280dp"
          android:layout_marginTop="38dp"
          android:layout_marginBottom="20dp"
          android:layout_toEndOf="@+id/namamobil"
          android:text="Tgl Kembali:"
          android:textSize="15dp"
          android:textStyle="bold"
          tools:ignore="TextSizeCheck" />

      <TextView
          android:id="@+id/namamobil"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignParentStart="true"
          android:layout_alignParentTop="true"
          android:layout_marginStart="27dp"
          android:layout_marginLeft="120dp"
          android:layout_marginTop="35dp"
          android:layout_marginBottom="20dp"
          android:text="Nama Mobil"
          android:textSize="15dp"
          android:textStyle="bold"
          tools:ignore="TextSizeCheck" />

      <TextView
          android:id="@+id/tuju"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignParentTop="true"
          android:layout_marginStart="131dp"
          android:layout_marginLeft="280dp"
          android:layout_marginTop="63dp"
          android:layout_marginBottom="20dp"
          android:layout_toEndOf="@+id/namamobil"
          android:text="Tujuan"
          android:textSize="15dp"
          tools:ignore="TextSizeCheck" />

      <TextView

          android:id="@+id/textView7"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignParentTop="true"
          android:layout_marginStart="41dp"
          android:layout_marginLeft="280dp"
          android:layout_marginTop="15dp"
          android:layout_marginBottom="20dp"
          android:layout_toEndOf="@+id/namamobil"
          android:text="Tgl Pinjam :"
          android:textSize="15dp"
          android:textStyle="bold"
          tools:ignore="TextSizeCheck" />

      <TextView

          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignParentTop="true"
          android:layout_marginStart="43dp"
          android:layout_marginLeft="280dp"
          android:layout_marginTop="61dp"
          android:layout_marginBottom="20dp"
          android:layout_toEndOf="@+id/namamobil"
          android:text="Tujuan        :"
          android:textSize="15dp"
          android:textStyle="bold"
          tools:ignore="TextSizeCheck" />

      <TextView
          android:id="@+id/pinjam"
          android:layout_width="112dp"
          android:layout_height="29dp"
          android:layout_alignParentTop="true"
          android:layout_marginStart="9dp"
          android:layout_marginTop="12dp"
          android:layout_toEndOf="@+id/textView7"
          android:text="Tanggal Kembali" />

      <TextView
          android:id="@+id/kembali"
          android:layout_width="112dp"
          android:layout_height="29dp"
          android:layout_alignParentTop="true"
          android:layout_marginStart="8dp"
          android:layout_marginTop="37dp"
          android:layout_toEndOf="@+id/textView7"
          android:text="Tanggal Pinjam" />

    </RelativeLayout>
  </androidx.cardview.widget.CardView>
</RelativeLayout>