<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AkunFragment"
    android:background="@drawable/ic_gradasibgbadan">

    <RelativeLayout
        android:id="@+id/inti"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="80dp"
        android:background="@drawable/rounded_bgputih"
        app:layout_constraintTop_toTopOf="parent">


        <RelativeLayout
            android:id="@+id/ubahdata"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="false"
            android:layout_alignParentBottom="false">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="50dp"
                android:src="@drawable/icon_account" />

            <ImageView
                android:id="@+id/btndataakun"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginLeft="350dp"
                android:layout_marginTop="50dp"
                android:src="@drawable/icon_arrow_forward" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="66dp"
                android:layout_marginStart="60dp"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="60dp"
                android:layout_marginBottom="545dp"
                android:text="Ubah Data Akun" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="100dp"
                android:background="#000000" />
        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/aboutMitra"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="120dp"
                android:src="@drawable/icon_info" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="66dp"
                android:layout_marginStart="60dp"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="130dp"
                android:layout_marginEnd="0dp"
                android:layout_marginBottom="475dp"
                android:text="Tentang Mitra Sempurna Jaya" />

            <ImageView
                android:id="@+id/btntentangmitra"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginLeft="350dp"
                android:layout_marginTop="120dp"
                android:src="@drawable/icon_arrow_forward" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="170dp"
                android:background="#000000" />
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="180dp"
                android:src="@drawable/icon_chat" />

            <View
                android:id="@+id/view"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="230dp"
                android:background="#000000" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="60dp"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="190dp"
                android:layout_marginEnd="0dp"
                android:layout_marginBottom="421dp"
                android:text="Pusat Bantuan" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_marginStart="60dp"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="190dp"
                android:layout_marginEnd="0dp"
                android:layout_marginBottom="421dp"
                android:text="Pusat Bantuan" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="230dp"
                android:background="#000000" />

            <ImageView
                android:id="@+id/btnpusatbantuan"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginLeft="350dp"
                android:layout_marginTop="180dp"
                android:src="@drawable/icon_arrow_forward" />

        </RelativeLayout>


        <ImageView
            android:layout_marginTop="240dp"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:src="@drawable/icon_directions" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/btnsyaratdnaketentuan"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginLeft="350dp"
                android:layout_marginTop="240dp"
                android:src="@drawable/icon_arrow_forward" />

            <View
                android:id="@+id/view5"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="290dp"
                android:background="#000000" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_marginStart="60dp"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="250dp"
                android:layout_marginEnd="0dp"
                android:layout_marginBottom="361dp"
                android:text="Syarat dan Ketentuan" />
        </RelativeLayout>

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>